{
  "name": "chrome-webstore-upload-cli",
  "version": "2.1.0",
  "description": "CLI tool to upload Chrome Extensions to the Chrome Web Store",
  "keywords": [
    "chrome",
    "publish",
    "webstore",
    "store",
    "extension",
    "deploy",
    "update",
    "upload",
    "cli"
  ],
  "repository": "fregante/chrome-webstore-upload-cli",
  "funding": "https://github.com/sponsors/fregante",
  "license": "MIT",
  "author": "Andrew Levine",
  "contributors": [
    "Federico Brigante <me@fregante.com> (https://fregante.com)"
  ],
  "type": "module",
  "bin": {
    "chrome-webstore-upload": "cli.js"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "xo": {
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "camelcase": "off",
      "unicorn/no-process-exit": "off"
    },
    "space": 4
  },
  "ava": {
    "files": [
      "!test/fixtures/**",
      "!test/helpers/**"
    ]
  },
  "dependencies": {
    "chrome-webstore-upload": "^0.5.0",
    "junk": "^4.0.0",
    "meow": "^10.1.1",
    "ora": "^6.0.1",
    "recursive-readdir": "^2.2.2",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "execa": "^5.1.1",
    "is-stream": "^3.0.0",
    "xo": "^0.44.0"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  }
}
